{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "math-admin",
  "version": "0.2.0",
  "identifier": "com.math.admin",
  "build": {
    "frontendDist": "../admin/root/dist",
    "devUrl": "http://127.0.0.1:5778",
    "beforeDevCommand": "pnpm dev:play",
    "beforeBuildCommand": "pnpm build:play"
  },
  "app": {
    "windows": [
      {
        "title": "math-admin",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
